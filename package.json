{
  "name": "satsuki-junketsu",
  "version": "2.0.1",
  "private": true,
  "homepage": "https://bakuzan.github.io/satsuki-junketsu/",
  "dependencies": {
    "classnames": "^2.2.6",
    "history": "^4.7.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dnd": "^7.0.2",
    "react-dnd-html5-backend": "^7.0.2",
    "react-dnd-touch-backend": "^0.8.0",
    "react-dom": "^16.8.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.8",
    "react-scripts": "2.1.5",
    "redux": "^3.7.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,scss}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit-msg && npm run lint"
    }
  },
  "scripts": {
    "lint": "lint-staged",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "start": "for /f %i in ('git rev-parse --abbrev-ref HEAD') do set REACT_APP_BRANCH=%i && react-scripts start",
    "build": "for /f %i in ('git rev-parse --abbrev-ref HEAD') do set REACT_APP_BRANCH=%i && react-scripts build",
    "eject": "react-scripts eject",
    "deploy": "npm run build&&gh-pages -d build",
    "storybook": "export NODE_PATH=src && start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "babel-core": "^6.26.0",
    "gh-pages": "^1.1.0",
    "pre-commit": "^1.2.2",
    "redux-logger": "^3.0.6"
  },
  "browserslist": [
    ">1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
