{
  "name": "satsuki-junketsu",
  "version": "3.0.0",
  "private": true,
  "homepage": "https://bakuzan.github.io/satsuki-junketsu/",
  "dependencies": {
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "history": "^4.10.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dnd-touch-backend": "^10.0.2",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "redux": "^4.0.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,scss}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "lint": "lint-staged",
    "start": "for /f %i in ('git rev-parse --abbrev-ref HEAD') do set REACT_APP_BRANCH=%i && react-scripts start",
    "build": "for /f %i in ('git rev-parse --abbrev-ref HEAD') do set REACT_APP_BRANCH=%i && react-scripts build",
    "eject": "react-scripts eject",
    "deploy": "npm run build && gh-pages -d build",
    "storybook": "export NODE_PATH=src && start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "babel-core": "^6.26.0",
    "gh-pages": "^2.2.0",
    "redux-logger": "^3.0.6"
  },
  "browserslist": [
    ">1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
